    "coverage": "vitest run --coverage",
    "update:yahoo": "npm i yahoo-finance2",
    "update:playwright": "npm install -D @playwright/test@latest",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
